html, body {
     width: 100%;
     height: 100%;
 }

.bg {
    width: 100%;
    height: 100%;
    background-image: url("../images/italy-banner.jpg");
    background-color: #282828;
    background-position-x: center;
    background-position-y: 50%;
    background-size: cover;
    filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='E:/html/images/italy-banner.jpg', sizingMethod='scale');
    display: none;
}

.nav-box {
    width: 100%;
    height: 70px;
    background-color: #0f131c;
    /*filter: alpha(opacity=50);
    opacity: 1;*/
    box-shadow: 0px 2px 10px #0f131c;
   /*background-color:rgba(25,27,35,.9)*/
}
.nav-bar{
    margin: 0 auto;
    width: 1190px;
}
.nav-bar .logo{
    width: 106px;
    height: 35px;
    float: left;
    line-height: 70px;
}
.nav-bar .logo a{}
.nav-bar .logo a img{}
.nav-bar .ul-list{
    width: 687px;
    height: 70px;
    line-height: 70px;
    float: left;
/*    background-color: #6e6e6e;*/
}
.nav-bar .ul-list .mr-l{margin-left: 40px;}
.nav-bar .ul-list li{
  margin-right: 29px;
  display: inline;
}
.nav-bar .ul-list li a{
    font-size: 22px;
    color: #dae1e5;
}
.nav-bar .ul-list li a:hover{
  text-decoration: none;
}
.nav-bar .getino{
    float: left;
    width: 397px;
    height: 70px;
    /*background-color:#e4f4ff ;*/
    position: relative;
}
.nav-bar .getino .search-box{
    width: 270px;
    height: 60px;
    background-color: #0f131c;
    border-radius: 10px;
    position: absolute;
    top:7px;
    left:32px;
}
.search-box .box-wrap{
   margin-left: 21px;
   margin-top: 13px;
}
.search-box .box-wrap .search-txt{
    width: 233px;
    height: 36px;
    background-color: #ffffff;
    border-radius: 20px;
    position: absolute;
    top:12px;
    outline: none;
    border:none
}
.search-box .box-wrap .search-img{display:none}
.search-box .box-wrap .search-btn{
    width: 30px;
    height: 30px;
    outline: none;
    background: none;
    background-image: url("../images/search-img.png");
    background-repeat: no-repeat;
    background-position-x: center;
    background-position-y: 5px;
    border-left:1px solid  #0f131c;
    position: absolute;
    top:15px;
    right:23px;
    border-top:none;
    border-right:none;
    border-bottom:none;

}
.getino .search-msg-login{
    float: right;
}
.getino .search-msg-login li{
    line-height: 70px;
    margin-left: 10px;
    padding: 0!important;
}
.getino .search-msg-login{}
.getino .search-msg-login li{display: inline-block}
.getino .search-msg-login li img{display: inline-block}
.getino .search-msg-login .search-ico{}
.getino .search-msg-login .msg{position: relative}
.getino .search-msg-login .msg .msg-ico{}
.getino .search-msg-login .msg .msg-tip{
    position: absolute;
    width: 14px;
    height: 14px;
    top: 16px;
    left: 25px;
    background-color:#f05959;
    border-radius: 50%;
    line-height: 14px;
    vertical-align: middle;
    text-align: center;
    font-size: 12px;
    color: #ffffff;
}

.getino .search-msg-login .login{}
.getino .search-msg-login .login-ico{}


    left:32px;
}
.search-box .box-wrap{
   margin-left: 21px;
   margin-top: 13px;
}
.search-box .box-wrap .search-txt{
    width: 233px;
    height: 36px;
    background-color: #ffffff;
    border-radius: 20px;
    position: absolute;
    top:12px;
    outline: none;
    border:none
}
.search-box .box-wrap .search-img{display:none}
.search-box .box-wrap .search-btn{
    width: 30px;
    height: 30px;
    outline: none;
    background: none;
    background-image: url("../images/search-img.png");
    background-repeat: no-repeat;
    background-position-x: center;
    background-position-y: 5px;
    border-left:1px solid  #0f131c;
    position: absolute;
    top:15px;
    right:23px;
    border-top:none;
    border-right:none;
    border-bottom:none;

}
.getino .search-msg-login{
    float: right;
}
